<+>{% extends 'nomina/base.html' %}\r\n{% load static %}\r\n{% load humanize %}\r\n{% load custom_filters %}\r\n{% block titulo %}Novedades{% endblock %}\r\n{% block contenedor %}\r\n\r\n<style>\r\n    .boton_regresar, .boton_mas {\r\n        background-color: #EBECE4;\r\n    }\r\n\r\n    .boton_regresar:hover, .boton_mas:hover {\r\n        background-color: #000000;\r\n        color: #EBECE4;\r\n    }\r\n\r\n\r\n    .contenedor-imagen-novedad {\r\n        padding: 0 !important;\r\n    }\r\n\r\n    .image-novedad {\r\n        width: 100%;\r\n        height: 100px;\r\n        object-fit: cover;\r\n    }\r\n\r\n    h2 {\r\n        text-align: center;\r\n    }\r\n\r\n    .card{\r\n        margin-top: 50px;\r\n        background-color: transparent !important;\r\n    }\r\n\r\n    table {\r\n        margin-top: 100px;\r\n        background-color: #EBECE4 !important;\r\n    }\r\n\r\n    td, th {\r\n        border: 1px solid #C4C7C1;\r\n        text-align: center;\r\n        white-space: nowrap;\r\n        padding: 20px;\r\n    }\r\n\r\n    th {\r\n        background-color: #442B1C;\r\n        color: #EBECE4;\r\n    }\r\n\r\n    .contenedor__tarjeta_novedad {\r\n        margin-top: 100px;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        gap: 50px;\r\n\r\n    }\r\n\r\n    .tarjeta_novedad {\r\n        width: 200px !important;\r\n        height: 300px;\r\n        border: 2px solid #000;\r\n        border-radius: 50px;\r\n        text-align: center;\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        justify-content: center;\r\n        background-color: #EBECE4;\r\n    }\r\n\r\n    .tarjeta_dias {\r\n        display: grid;\r\n        grid-template-columns: auto auto;\r\n        gap: 5px;\r\n    }\r\n\r\n    .dia_novedad {\r\n        border: 2px solid #000;\r\n        padding: 10px 13px;\r\n        border-radius: 50px;\r\n        color: #EBECE4;\r\n        background-color: #000;\r\n        font-size: 20px;\r\n    }\r\n\r\n    .dia_novedad:hover {\r\n        border: 2px solid #000 !important;\r\n    }\r\n</style>\r\n\r\n<body style=\"background-color: #442B1C;\">\r\n\r\n<nav class=\"navbar navbar-expand-lg navbar-light\">\r\n    <div class=\"container-fluid d-flex justify-content-between align-items-center\">\r\n        <a class=\"btn btn-light transition duration-300 ease-in-out boton_regresar\" href=\"{% url 'nomina:index' %}\">\r\n            Regresar <i class=\"fa-solid fa-reply\"></i>\r\n        </a>\r\n        <h1 class=\"h3 mb-2 text-gray-800\" style=\"color: #EBECE4;\">Novedades</h1>\r\n        <button class=\"btn rounded btn-light boton_mas\" data-bs-toggle=\"modal\" data-bs-target=\"#novedadModal\">\r\n            Crear Novedad\r\n        </button>\r\n    </div>\r\n</nav>\r\n\r\n<div class=\"modal fade\" id=\"novedadModal\" tabindex=\"-1\" aria-labelledby=\"novedadModalLabel\"\r\n     aria-hidden=\"true\">\r\n    <div class=\"modal-dialog modal-lg\">\r\n        <div class=\"modal-content\">\r\n            <div class=\"modal-header\">\r\n                <h1 class=\"modal-title fs-5\" id=\"novedadModalLabel\">Crear Novedad</h1>\r\n                <button type=\"button\" class=\"btn btn-close\" data-bs-dismiss=\"modal\"\r\n                        aria-label=\"Close\">\r\n                </button>\r\n            </div>\r\n            <div class=\"modal-body\">\r\n                <form action=\"{% url 'nomina:novedad_guardar' %}\" method=\"post\">\r\n                    {% csrf_token %}\r\n                    <div id=\"step1\">\r\n                        <p><strong style=\"color:red;\">*</strong>: Obligatorio</p>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Usuario: <strong style=\"color:red;\">*</strong></label>\r\n                            <select class=\"form-control\" name=\"usuario\" required>\r\n                                <option value=\"\">Seleccionar...</option>\r\n                                {% for usuario in usuarios %}\r\n                                <option value=\"{{ usuario.id }}\">{{ usuario }}</option>\r\n                                {% endfor %}\r\n                            </select>\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Fecha Inicio Novedad: <strong style=\"color:red;\">*</strong></label>\r\n                            <input id=\"fecha_inicio_novedad\" type=\"date\" class=\"form-control\" name=\"fecha_inicio\" required>\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Fecha Fin Novedad: <strong style=\"color:red;\">*</strong></label>\r\n                            <input id=\"fecha_fin_novedad\" type=\"date\" class=\"form-control\" name=\"fecha_fin\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Salario: <strong style=\"color:red;\">*</strong></label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"salario\" required>\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Días de Incapacidad:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"dias_incapacidad\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Días Trabajados: <strong style=\"color:red;\">*</strong></label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"dias_trabajados\" required>\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Extras Diurnas:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"horas_extras_diurnas\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Extras Diurnas Dom/Fes:</label>\r\n                            <input type=\"number\" class=\"form-control\"\r\n                                   name=\"horas_extras_diurnas_dom_fes\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Extras Nocturnas:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"horas_extras_nocturnas\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Extras Nocturnas Dom/Fes:</label>\r\n                            <input type=\"number\" class=\"form-control\"\r\n                                   name=\"horas_extras_nocturnas_dom_fes\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Recargo Nocturno:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"horas_recargo_nocturno\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Recargo Nocturno Dom/Fes:</label>\r\n                            <input type=\"number\" class=\"form-control\"\r\n                                   name=\"horas_recargo_nocturno_dom_fes\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Horas Recargo Diurno Dom/Fes:</label>\r\n                            <input type=\"number\" class=\"form-control\"\r\n                                   name=\"horas_recargo_diurno_dom_fes\">\r\n                        </div>\r\n                        <button type=\"button\" class=\"btn btn-secondary\" id=\"nextStep\">Siguiente</button>\r\n                    </div>\r\n\r\n                    <div id=\"step2\" style=\"display: none;\">\r\n                        <p><strong style=\"color:red;\">*</strong>: Obligatorio</p>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Comisiones:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"comisiones\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Comisiones Porcentaje:</label>\r\n                            <input type=\"number\" step=\"0.01\" class=\"form-control\"\r\n                                   name=\"comisiones_porcentaje\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Embargos Judiciales:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"embargos_judiciales\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Libranzas:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"libranzas\">\r\n                        </div>\r\n                        <div class=\"mb-3\">\r\n                            <label class=\"form-label\">Otros:</label>\r\n                            <input type=\"number\" class=\"form-control\" name=\"otros\">\r\n                        </div>\r\n                        <button type=\"button\" class=\"btn btn-secondary\" id=\"prevStep\">Anterior</button>\r\n                        <button type=\"submit\" class=\"btn btn-primary\">Crear</button>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<!-- Contenido principal -->\r\n<div class=\"contenedor__tarjeta_novedad wow fadeInUp\">\r\n    {% for mes, novedades in novedades_por_mes.items %}\r\n    <div class=\"tarjeta_novedad\">\r\n        <h2>{{ mes|date:\"F\"|capfirst }}</h2>\r\n        <div class=\"tarjeta_dias\">\r\n        {% for i in novedades %}\r\n            <button class=\"btn dia_novedad\" data-dia=\"{{ i.fecha_fin|date:'d' }}\" data-mes=\"{{ mes|date:'m' }}\" data-year=\"{{ mes|date:'Y' }}\">{{ i.fecha_fin|date:\"d\" }}</button>\r\n        {% endfor %}\r\n        </div>\r\n    </div>\r\n    {% endfor %}\r\n</div>\r\n<div class=\"card shadow mb-4\">\r\n    <div class=\"card-body\">\r\n        <div class=\"table-responsive\">\r\n            <div class=\"contenedor__tabla_novedad\">\r\n            <table class=\"mb-5\" id=\"dataTable\" width=\"100%\" cellspacing=\"0\">\r\n                <thead>\r\n                <tr>\r\n                    <th></th>\r\n                    <th>Foto</th>\r\n                    <th>Fecha novedad</th>\r\n                    <th>Colaborador</th>\r\n                    <th>Salario</th>\r\n                    <th>Días de Incapacidad</th>\r\n                    <th>Días Trabajados</th>\r\n                    <th>H.E. Diurnas</th>\r\n                    <th>H.E. Diurnas Dom/Fes</th>\r\n                    <th>H.E. Nocturnas</th>\r\n                    <th>H.E. Nocturnas Dom/Fes</th>\r\n                    <th>H.R. Nocturno</th>\r\n                    <th>H.R. Nocturno Dom/Fes</th>\r\n                    <th>H.R. Diurno Dom/Fes</th>\r\n                    <th>Comisiones</th>\r\n                    <th>Comisiones (%)</th>\r\n                    <th>Bonificaciones</th>\r\n                    <th>Embargos Judiciales</th>\r\n                    <th>Libranzas</th>\r\n                    <th>Cooperativas</th>\r\n                    <th>Otros</th>\r\n                </tr>\r\n                </thead>\r\n                <tbody>\r\n                {% for i in novedades %}\r\n                <tr>\r\n                    <td>\r\n                        <button class=\"btn\" id=\"abrirModalEditarNovedad\" data-bs-toggle=\"modal\" data-bs-target=\"#editarModalNovedad{{ i.id }}\"><i class=\"bi bi-pencil-fill\"></i></button>\r\n                        <a href=\"javascript:confirmar_eliminar('{{ i.id }}', '{% url 'nomina:novedad_eliminar' i.id %}');\" class=\"btn\"><i class=\"bi bi-trash-fill\"></i></a>\r\n                        <a href=\"{% url 'nomina:nomina_listar' i.fecha_inicio i.fecha_fin %}\" class=\"btn\">Ver nómina</a>\r\n                    </td>\r\n                    <td class=\"contenedor-imagen-novedad\"><img class=\"image-novedad\" src=\"{{ i.usuario.foto.url }}\" alt=\"Foto de {{ i.usuario }}\"></td>\r\n                    <td>{{ i.fecha_fin }}</td>\r\n                    <td>{{ i.usuario }}</td>\r\n                    <td>{{ i.salario|intcomma }}</td>\r\n                    <td>{{ i.dias_incapacidad|default:\"\" }}</td>\r\n                    <td>{{ i.dias_trabajados }}</td>\r\n                    <td>{{ i.horas_extras_diurnas|default:\"\" }}</td>\r\n                    <td>{{ i.horas_extras_diurnas_dom_fes|default:\"\" }}</td>\r\n                    <td>{{ i.horas_extras_nocturnas|default:\"\" }}</td>\r\n                    <td>{{ i.horas_extras_nocturnas_dom_fes|default:\"\" }}</td>\r\n                    <td>{{ i.horas_recargo_nocturno|default:\"\" }}</td>\r\n                    <td>{{ i.horas_recargo_nocturno_dom_fes|default:\"\" }}</td>\r\n                    <td>{{ i.horas_recargo_diurno_dom_fes|default:\"\" }}</td>\r\n                    {% if i.comisiones %}\r\n                        <td>${{ i.comisiones|default:\"\"|intcomma }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                    <td>{{ i.comisiones_porcentaje|default:\"\" }}</td>\r\n\r\n                    {% if i.bonificaciones %}\r\n                        <td>${{ i.bonificaciones|default:\"\"|intcomma }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                    {% if i.embargos_judiciales %}\r\n                        <td>${{ i.embargos_judiciales|default:\"\"|intcomma }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                    {% if i.libranzas %}\r\n                        <td>${{ i.libranzas|default:\"\"|intcomma }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                    {% if i.cooperativas %}\r\n                        <td>${{ i.cooperativas|default:\"\"|intcomma }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                    {% if i.otros %}\r\n                        <td>${{ i.otros|default:\"\"|intcomma }}</td>\r\n                    {% else %}\r\n                        <td></td>\r\n                    {% endif %}\r\n                </tr>\r\n                <div class=\"modal fade\" id=\"editarModalNovedad{{ i.id }}\" tabindex=\"-1\" aria-labelledby=\"editarModalNovedadLabel{{ i.id }}\"\r\n                         aria-hidden=\"true\">\r\n                        <div class=\"modal-dialog modal-lg\">\r\n                            <div class=\"modal-content\">\r\n                                <div class=\"modal-header\">\r\n                                    <h1 class=\"modal-title fs-5\" id=\"editarModalNovedadLabel{{ i.id }}\">Editar Colaborador</h1>\r\n                                    <button type=\"button\" class=\"btn btn-close\" data-bs-dismiss=\"modal\"\r\n                                            aria-label=\"Close\">\r\n                                    </button>\r\n                                </div>\r\n                                <div class=\"modal-body\" id=\"contenidoModalBody\">\r\n                                    {% include 'nomina/novedad/novedades_editar.html' with data=i %}\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                {% endfor %}\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/npm/flatpickr/dist/flatpickr.min.css\">\r\n<script src=\"https://cdn.jsdelivr.net/npm/flatpickr\"></script>\r\n<script>\r\n    document.addEventListener('DOMContentLoaded', (event) => {\r\n        const step1 = document.getElementById('step1');\r\n        const step2 = document.getElementById('step2');\r\n        const nextStep = document.getElementById('nextStep');\r\n        const prevStep = document.getElementById('prevStep');\r\n\r\n        nextStep.addEventListener('click', () => {\r\n            step1.style.display = 'none';\r\n            step2.style.display = 'block';\r\n        });\r\n\r\n        prevStep.addEventListener('click', () => {\r\n            step1.style.display = 'block';\r\n            step2.style.display = 'none';\r\n        });\r\n    });\r\n\r\n    document.addEventListener(\"DOMContentLoaded\", function() {\r\n        // Eliminar duplicados de días en la tarjeta de novedad\r\n        document.querySelectorAll('.tarjeta_novedad').forEach(function(tarjeta) {\r\n            let diasUnicos = new Set();\r\n\r\n            tarjeta.querySelectorAll('.dia_novedad').forEach(function(boton) {\r\n                let dia = boton.textContent.trim();\r\n\r\n                if (diasUnicos.has(dia)) {\r\n                    boton.style.display = 'none';\r\n                } else {\r\n                    diasUnicos.add(dia);\r\n                }\r\n            });\r\n        });\r\n    });\r\n\r\n\r\n</script>\r\n\r\n{% endblock %}
